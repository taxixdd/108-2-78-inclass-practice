taiwan_time <- "西元2019年4月12日 12點55分30秒"
oslo_time <- "18.11.2018 15:33:02"
# two_times
library(lubridate)
taiwan_time <- ymd_hms(taiwan_time,
tz="Asia/Taipei")
oslo_time <- dmy_hms(oslo_time,
tz="Europe/Oslo")
two_times <- c(taiwan_time,oslo_time)
# two_times_chicago
two_times_chicago <- c(with_tz(taiwan_time, tzone="America/Chicago"),with_tz(oslo_time, tzone="America/Chicago")
)
